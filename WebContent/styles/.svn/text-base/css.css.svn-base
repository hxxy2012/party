/*悬浮层*/
.layer { height:50px; position:fixed; right:10px; bottom:50px; z-index:4; box-sizing: inherit}
.layer_menu { width:50px; height:50px; position:absolute; bottom:0px; right:0px; z-index:5; border-radius:50%;}
.layer_menu .black_bg { width:50px; height:50px; padding:5px; float:left; margin-right:10px; background-color:#000; filter:alpha(opacity=50);  -moz-opacity:0.5;  -khtml-opacity: 0.5;  opacity: 0.5; border-radius:50%;}
.layer_menu img {position:absolute; top:7px; right:7px; z-index:1;}
.layer_show { width:120px; height:50px; padding-right:50px; border-radius:25px; position:absolute; right:0px; bottom:0px;}
.show_bg {width:200px; height:50px; border-radius:25px; position:absolute; right:0px; bottom:0px; background-color:#000;filter:alpha(opacity=50);  -moz-opacity:0.5;  -khtml-opacity: 0.5;  opacity: 0.5; z-index:4;}
.layer_show ul {width:100px; height:40px; padding:5px 0px; position:absolute; left:-50px; bottom:8px; z-index:5;}
.layer_show ul li { width:50%; float:left; display:block; text-align:center;}
.layer_show ul li a { width:100%; color:#fff; float:left;}
.layer_show ul li a p { font-size:12px;}

