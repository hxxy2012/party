body{font-family:"Microsoft Yahei",Verdana,Arial,Helvetica,sans-serif;font-size:1em;line-height:1.25em;color:#2b2b2b}
.header{height:56px;/* background:#55a922; */
/*position:relative;display:table;width:100%}*/
/* .header .logo{display:table-cell;height:100%;vertical-align:middle}
.header .logo .logo_img{padding-left:10px;width:130px} */
.form-search{display:table-cell;text-align:left;vertical-align:middle;height:100%;width:58%;padding-left:10px}
.form-search .inp-search{background:transparent;height:36px;border:0;padding:0;border-radius:8px;box-sizing:border-box;padding-left:0.8em;background:#7abc53;color:#fff;width:100%;font-family:"Microsoft Yahei"}
