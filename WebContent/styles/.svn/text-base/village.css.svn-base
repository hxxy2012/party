.header .go_parent{position:absolute;left:10px;top:12px;background:url(../images/back.png) no-repeat;width:32px;height:32px;background-size:100%}
.header .share{position:absolute;right:10px;top:12px;background:url(../images/share.png) no-repeat;width:32px; height:24px;background-size:100%; color:#fff}
.header .sch_btn{position:absolute;right:10px;top:12px;color:#fff;line-height:28px;font-size:16px}
.header .sch_btn span{background:url(../images/search.png) no-repeat;width:28px; float:left;height:28px;margin-right:4px;margin-top:2px}
.header .sch_btn a{color:#fff}
.header .note{position:absolute;right:10px;top:12px;background:url(../images/note.png) no-repeat;width:32px; height:32px;background-size:100%}
.header .around{position:absolute;right:10px;top:14px;line-height:20px;font-size:1rem}
.header .around .ico{float:left;margin-right:8px;background:url(../images/around.png) no-repeat;width:14px;height:21px;background-size:100%}
.header .around .there{background-image:url(../images/there.png);width:19px;height:19px}
.main-nav{padding-left:10px;line-height:38px;position:relative;overflow:hidden}
.main-nav ul{width:1000px;margin-left: 0px;}
.main-nav ul:before,.main-nav ul:after{display:table;content:"";line-height:0}.main-nav ul:after{clear:both}
.main-nav li{float:left;font-size:18px;margin-right:5px}
.main-nav .all_menu{text-indent:-30em;overflow:hidden;background:#fff url(../images/all_menu.png) no-repeat 0 center;width:35px;position:absolute;top:0;right:0}
.slider{clear:both;position:relative;overflow:hidden;}
.slide-banner{position:relative}
.slide-banner li{float:left}
.slide-banner a{display:block;overflow:hidden;line-height:0}
.slide-banner img{width:100%}
.indicator{bottom:0;height:30px;line-height:30px;overflow:hidden;position:absolute;z-index:100;background:rgba(0,0,0,0);width:100%}
.indicator .topic-title{float:left;color:#fff;height:100%;overflow:hidden;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box; max-width:79.5%}
.indicator .topic-title .infor{background:#fe0000;display:inline-block;float:left;height:22px;line-height:22px;width:40px;text-align:center;color:#fff;margin-top:4px;margin-right:8px}
.indicator .topic-title .infor em{font-weight:bold}
.indicator .topic-title a{color:#FFF}
.indicator-num{float:right;padding-right:10px}
.point{border-radius:7px;-webkit-border-radius:7px;-ms-border-radius:7px;height:7px;width:7px;background:#fff;/*margin-right:7px;*/display:inline-block;vertical-align:middle}
.point.selected{background:#55a922}

.container{/*margin-top:6px*/}
