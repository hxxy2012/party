.titlebar{height:50px;line-height:50px;margin:3px 0px 6px 0px;}
*/
.titlebar{height:40px;line-height:40px;}
.titlebar h4{font-size:18px;margin-left:10px;overflow:hidden;font-weight:bold;float:left}
.titlebar h4:before{content:"";display:block;width:3px;height:14px;float:left;background:#55a922;margin:9px 7px 0 0}
.titlebar .more{float:right;padding-right:10px}
.titlebar .more a{color:#7f8180}
.ico-right{margin-left:4px;background:url(../images/more_r.png) no-repeat;background-size:100%;width:17px;height:17px;vertical-align:middle;display:inline-block}
.module-c{overflow:hidden;padding:0 10px}
.video-row{margin-left:-10px;overflow:hidden}
.videolist{width:100%}
.videolist li{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;float:left;padding-left:10px; width:50%}
.videolist li a{color:#666;font-size:14px;display:block;width:100%;line-height:0;overflow:hidden;position:relative}
.videolist li img{width:100%}
.videolist .v-thumb{color:white;height:26px;line-height:26px;overflow:hidden;padding:0 5px 0 8px;text-overflow:ellipsis;white-space:nowrap;width:100%;position:absolute;bottom:0;left:0;-webkit-box-sizing:border-box;text-align:center}
.videolist .v-thumb .play{background:url(../images/play.png) no-repeat;width:22px;height:22px;float:left}
.videolist .v-thumb .duration{float:right;background-color:rgba(0,0,0,0.6);font-size:12px;color:#fff;height:18px;margin-top:2px;line-height:18px;padding:0 3px}
.v-title{display:block;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;line-height:1.5rem;margin:4px 0 1px}
.videolist .num{font-size:14px;color:#666;margin-bottom:9px}
